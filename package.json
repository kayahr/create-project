{
    "$schema": "http://json.schemastore.org/package",
    "name": "@kayahr/create-project",
    "version": "2.0.0",
    "type": "module",
    "description": "Template for kayahr typescript projects",
    "keywords": [
        "typescript",
        "kayahr",
        "template"
    ],
    "license": "MIT",
    "author": "Klaus Reimer <k@ailis.de>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kayahr/create-project.git"
    },
    "homepage": "https://github.com/kayahr/create-project",
    "bugs": "https://github.com/kayahr/create-project/issues",
    "funding": "https://github.com/kayahr/create-project?sponsor=1",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "lib/main/",
        "src/main/",
        "template"
    ],
    "bin": {
        "create-project": "lib/main/create-project.js"
    },
    "scripts": {
        "clean": "rm -rf lib",
        "build": "run build:*",
        "build:compile": "tsc",
        "test": "run test:*",
        "test:spell": "cspell --no-progress --no-summary",
        "test:lint": "oxlint --type-aware --deny-warnings",
        "start": "node lib/main/create-project.js",
        "prepare": "run clean build"
    },
    "devDependencies": {
        "@kayahr/assert": "1.3.2",
        "@kayahr/cspell": "9.2.2-bundle.1",
        "@kayahr/npm-utils": "1.1.1",
        "@kayahr/ntest": "1.3.0",
        "@kayahr/oxlint-config": "1.12.0",
        "@kayahr/typedoc": "0.28.14-bundle.2",
        "@types/node": "24.10.0",
        "typescript": "5.9.3"
    }
}
